{
  "name": "client",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.config.js --mode development --open 'firefox'",
    "test:frontend": "mocha 'src/**/*.test.js' --compilers js:babel-register --exit",
    "test2": "mocha 'src/**/*int2.test.js'",
    "test3": "mocha 'src/**/*int3.test.js'",
    "test4": "jest 'src/**/*int2.test.js'",
    "test": "jest",
    "build:deploy": "webpack -p"
  },
  "author": "",
  "license": "ISC",
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-2"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.15.0",
    "enzyme": "^3.4.2",
    "enzyme-adapter-react-16": "^1.2.0",
    "ignore-styles": "^5.0.1",
    "moxios": "^0.4.0",
    "react-hot-loader": "^4.3.4",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "css-loader": "^1.0.0",
    "jest": "^23.6.0",
    "jsdom": "^11.12.0",
    "promise.prototype.finally": "^3.1.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "style-loader": "^0.22.1",
    "webpack": "^4.19.0"
  },
  "engines": {
    "node": "8.11.2"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/components/**/*.js"
    ],
    "coverageDirectory": "./test/reports",
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "setupFiles": [
      "<rootDir>/test/setupTests.js"
    ]
  }
}
